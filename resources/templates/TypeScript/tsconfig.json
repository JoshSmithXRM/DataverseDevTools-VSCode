{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./WebResources/js",
        "sourceMap": true,
        "strict": true,
        "moduleResolution": "node",
        "removeComments": false,
        "allowJs": true,
        "resolveJsonModule": true
    },
    "include": [
        "./typings/**/*",
        "./src/**/*"
    ]
}
